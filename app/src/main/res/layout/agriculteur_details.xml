<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".agri.addAgriculteursFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10px"
        android:orientation="vertical">


        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="2dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="8dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/demo_pic_f_details"
                        android:layout_width="40px"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/ic_person_black_24dp"
                        tools:ignore="VectorDrawableCompat" />

                    <TextView
                        android:id="@+id/demo_f_details"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginLeft="10px"
                        android:gravity="left"
                        android:text="Information generale"
                        android:textAppearance="?android:attr/textAppearanceSmallInverse"
                        android:textColor="@color/grey_600"
                        android:textSize="20dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1px"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:layout_marginLeft="10px"
                        android:layout_weight="2"
                        android:background="@color/cardview_shadow_end_color"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/household_member_f_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Nom"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/household_member_f_details1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="xxx" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:layout_marginLeft="10px"
                        android:layout_weight="2"
                        android:background="@color/cardview_shadow_end_color"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/spouse_f_name_f_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Prenom"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/spouse_f_name_f_details1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="xxx" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:layout_marginLeft="10px"
                        android:layout_weight="2"
                        android:background="@color/cardview_shadow_end_color"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/spouse_last_name_f_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Sexe"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/spouse_last_name_f_details1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="M" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:layout_marginLeft="10px"
                        android:layout_weight="2"
                        android:background="@color/cardview_shadow_end_color"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/cell_phone_alt_f_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Telephone"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/cell_phone_alt_f_details1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_margin="2dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="123456798" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:layout_marginLeft="10px"
                        android:layout_weight="2"
                        android:background="@color/cardview_shadow_end_color"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/cell_carrier_f_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Adresse"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/cell_carrier_f_details1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Rutchuru" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:layout_weight="2"
                        android:background="@color/cardview_shadow_end_color"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="Validation"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/pic_irrigation_details"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="right|center_vertical|center_horizontal|center"
                            app:srcCompat="@drawable/ic_lock_outline_black_24dp"
                            tools:ignore="VectorDrawableCompat" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>


    </LinearLayout>
</RelativeLayout><!--
</android.support.v4.widget.NestedScrollView>
-->
